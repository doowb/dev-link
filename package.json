{
  "name": "dev-link",
  "description": "Link and run development versions of command line applications.",
  "version": "0.1.0",
  "homepage": "https://github.com/doowb/dev-link",
  "author": "Brian Woodward (https://github.com/doowb)",
  "repository": "doowb/dev-link",
  "bugs": {
    "url": "https://github.com/doowb/dev-link/issues"
  },
  "license": "MIT",
  "files": [
    "cli.js"
  ],
  "main": "cli.js",
  "bin": {
    "dev": "./cli.js"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "data-store": "^0.15.5",
    "pkg-store": "^0.2.1",
    "spawn-commands": "^0.3.1"
  },
  "devDependencies": {
    "gulp-format-md": "^0.1.7"
  },
  "verb": {
    "run": true,
    "toc": false,
    "layout": "global",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "reflinks": [
      "verb",
      "assemble"
    ],
    "lint": {
      "reflinks": true
    }
  },
  "keywords": [
    "app",
    "apps",
    "bin",
    "dev",
    "global apps",
    "link",
    "link app",
    "link global",
    "run app",
    "run global",
    "run"
  ]
}
